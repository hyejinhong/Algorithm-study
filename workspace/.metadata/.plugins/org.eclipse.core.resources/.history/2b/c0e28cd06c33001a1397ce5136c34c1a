	}
	
	// 완전 탐색 재귀로 풀기
	public static boolean jump(int y, int x) {
		// 기저1: 게임판 밖이야 !!
		if(y >= n || x >= n) {
			return false;
		}
		// 기저2: 마지막 칸
		if(y == n-1 && x == n-1) {
			return true;
		}
		
		int jumpSize = board[y][x];
		return jump(y+jumpSize, x) || jump(y, x+jumpSize);
	}

}
