package boj;

import java.util.*;

public class BOJ_1018 {

	static int n;
	static int m;
	static String[] board = new String[50];
	static int result = Integer.MAX_VALUE;
	final static int SIZE = 8;
	
	// 비교를 위한 보드판
	final static String[] black = {
			"BWBWBWBW",
			"WBWBWBWB",
			"BWBWBWBW",
			"WBWBWBWB",
			"BWBWBWBW",
			"WBWBWBWB",
			"BWBWBWBW",
			"WBWBWBWB",
	};
	
	final static String[] white = {
			"WBWBWBWB",
			"BWBWBWBW",
			"WBWBWBWB",
			"BWBWBWBW",
			"WBWBWBWB",
			"BWBWBWBW",
			"WBWBWBWB",
			"BWBWBWBW",	
	};
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		n = scan.nextInt();
		m = scan.nextInt();
		scan.nextLine();
		
		System.out.println("n: "+n+", m: "+m);
		
		for(int i=0; i<n; i++) {
			board[i] = scan.nextLine().trim();
		}
//		System.out.println(Arrays.toString(board));
//		System.out.println(solve(0, 0));
		solve(0, 0);
	}
	
	public static int solve(int y, int x) {
		for(int i=0; i<n; i++) {
			for(int j=0; j<m; j++) {
				result = Math.min(result, calculate(y+i, x+j));
				String str = ("y: " + (y+i) + ", x: " + (x+j) + ", result: " + result);
//				System.out.println(result);
				System.out.println(str);
			}
		}
		return result;
	}
	
	public static int calculate(int y, int x) {
//		if(y+SIZE-1 == n && x+SIZE-1 == m) {
//			return result;
//		}
		// 기저: 범위 초과
		if(y+SIZE-1 >= n || x+SIZE-1 >= m) {
			return Integer.MAX_VALUE;
		}
		
		// black 검사
		int blackNum = 0;
		for(int i=y; i<SIZE; i++) {
			for(int j=x; j<SIZE; j++) {
				char now = board[i].charAt(j);
				char com = black[i].charAt(j);
				if(board[i].charAt(j) != black[i].charAt(j)) {
					blackNum++;
				}
			}
		}
		
		// white 검사
		int whiteNum = 0;
		for(int i=y; i<SIZE; i++) {
			for(int j=x; j<SIZE; j++) {
				if(board[i].charAt(j) != white[i].charAt(j)) {
					whiteNum++;
				}
			}
		}
		
		int ret = Math.min(blackNum, whiteNum);
		return ret;
	}
}
