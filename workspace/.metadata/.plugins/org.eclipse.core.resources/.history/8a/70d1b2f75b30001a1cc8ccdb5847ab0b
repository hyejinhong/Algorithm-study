package project;

import java.util.*;

public class QuadTree {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		int c = scan.nextInt();
		
		// »óÇÏ¹ÝÀü
		// 1 -> 3
		// 2 -> 4
		// 3 -> 1
		// 4 -> 2
		scan.nextLine();
		for(int test=1; test<=c; test++) {
			String str = scan.nextLine();
			String result = reverse(str);
			System.out.println(result);
		}
	}
	
	// ¾ÐÃàÀ» Çª´Â ÇÔ¼ö
	public static String reverse(String str) {
		if(str.charAt(0) == 'b' || str.charAt(0) == 'w') {
			System.out.println("Äûµæ: " + str.substring(0, 1));
			return str.substring(0, 1);
		}
		
		String upperLeft = reverse(str.substring(1));
		String upperRight = reverse(str.substring(1));
		String lowerLeft = reverse(str.substring(1));
		String lowerRight = reverse(str.substring(1));
		
		return "x" + lowerLeft + lowerRight + upperLeft + upperRight;
	}
}
