package project;

import java.util.*;
public class BOJ_10827 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		double a = scan.nextDouble();
		int b = scan.nextInt();
		
		double result = Math.pow(b, a);
		System.out.println(result);
	}

}
