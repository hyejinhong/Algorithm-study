package project;

import java.util.*;

public class AllPermutation_10974 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		
		boolean[] checkArr = new boolean[n]; // 원소 사용되었는지 아닌지 체크
		
		System.out.println(Arrays.toString(checkArr));
//		for(int i=0; i<n; i++) {
//			arr[i] = i+1;
//			checkArr[i] = false;
//		}
//		
//		pick(arr, checkArr);
		
	}
	
	public static boolean isAllUsed(boolean[] checkArr) {
		for(int i=0; i<checkArr.length; i++) {
			if(!checkArr[i]) {
				return false;
			}
		}
		return true;
	}
	
	public static void pick(int[] arr, boolean[] checkArr) {
		// 기저 사례: 모든 원소가 사용되었으면 return
		if(isAllUsed(checkArr)) {
			return;
		}
		
		
	}
}
